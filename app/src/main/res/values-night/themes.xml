<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Base.Theme.Popipa" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/darkOrange</item>
        <item name="colorPrimaryVariant">@color/darkSalmon</item>
        <item name="colorOnPrimary">@color/darkBlue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkBlue</item>
        <item name="colorSecondaryVariant">@color/darkPink</item>
        <item name="colorOnSecondary">@color/darkPurple</item>
